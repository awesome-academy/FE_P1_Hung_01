extends ../index

block link
  link(rel="stylesheet", href="../../css/style.css")
  link(rel="stylesheet", href="../../css/header.css")
  link(rel="stylesheet", href="../../css/footer.css")
  link(rel="stylesheet", href="../../fontawesome-free-5.13.0-web/css/all.min.css")
  link(rel="stylesheet", href="../../css/products/path.css")
  link(rel="stylesheet", href="../../css/products/navbar.css")
  link(rel="stylesheet", href="../../css/products/main.css")

block main
  main
    include ./path
    include ./navbar
    include ./main
    
block li
  for n in dropdownFooter5
    case n 
      when "Tài khoản"
        li
          a(href="../account/account.html")=n
      when "Đăng ký"
        li
          a(href="../auth/register.html")=n
      when "Đăng nhập"
        li
          a(href="../aut/login.html")=n
      default
        li
          a(href="#")=n